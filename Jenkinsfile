                echo 'Build World'
            }
        }
                stage('Test my project') {
            steps {
                echo 'Test World'
            }
        }
                stage('Deploy my project') {
            steps {
                echo 'Deploy World'
            }
        }
                stage('Deploy2') {
            steps {
                echo 'Deploy World'
            }
        }
                stage('Deploy3') {
            steps {
                echo 'Deploy World'
            }
        }
    }
